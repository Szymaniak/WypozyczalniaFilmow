@model IEnumerable<WypozyczalniaFilmowa.Models.Kategoria>
<header>
    @foreach(var kategoria in Model)
            {
                <a href="@Url.Action("Lista", "Filmy", new { nazwaKategorii = kategoria.Nazwa })">@kategoria.Nazwa</a>
            }
    <a class="menu-link" href="@Url.Action("Wszystkie","Filmy")">Wszystkie</a>
</header>